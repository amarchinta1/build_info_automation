{
"hints":{
		"year":{
			"xpath":"//script[contains(@type,'text/javascript')][14]",
			"regex":"year:\\s('\\d{4}')"
		},
		"make":{
			"xpath":"//script[contains(@type,'text/javascript')][14]",
			"regex":"make:\\s('\\w+')"
		},
		"model":{
			"xpath":"//script[contains(@type,'text/javascript')][14]",
			"regex":"model:\\s('\\w+')"
		},
		"trim":{
			"xpath":"//div[contains(@class,'ddc-content tracking-ddc-data-layer')]",
			"regex": "\"trim\"\\:\\s(\"\\w+\")"
		}
	}
}
